        },
        {
          "name": "JWT_KEY",
          "valueFrom": "arn:aws:ssm:us-east-2:765170299946:parameter/JWT_KEY"
        }
      ]
    }
  ],
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::765170299946:role/ecsTaskExecutionRole",
  "family": "tv-minder",
  "memory": "512",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"]
