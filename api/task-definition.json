{
  "containerDefinitions": [
    {
      "command": null,
      "entryPoint": null,
      "essential": true,
      "image": "765170299946.dkr.ecr.us-east-2.amazonaws.com/tv-minder-repo:6930a27d1d8c3c8888fed0ec3f19fc7bf2e5cbcf",
      "name": "tv-minder-container",
      "logConfiguration": null,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
